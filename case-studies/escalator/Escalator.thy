theory Escalator imports Complex_Main
begin

type_synonym variable = nat
type_synonym process = nat
type_synonym pstate = nat

abbreviation userAtTop' :: variable where "userAtTop' \<equiv> (Suc 0)"
abbreviation userAtBottom' :: variable where "userAtBottom' \<equiv> (Suc (Suc 0))"
abbreviation directionSwitch' :: variable where "directionSwitch' \<equiv> (Suc (Suc (Suc 0)))"
abbreviation alarmButton' :: variable where "alarmButton' \<equiv> (Suc (Suc (Suc (Suc 0))))"
abbreviation stuck' :: variable where "stuck' \<equiv> (Suc (Suc (Suc (Suc (Suc 0)))))"
abbreviation up' :: variable where "up' \<equiv> (Suc (Suc (Suc (Suc (Suc (Suc 0))))))"
abbreviation down' :: variable where "down' \<equiv> (Suc (Suc (Suc (Suc (Suc (Suc (Suc 0)))))))"
abbreviation direction' :: variable where "direction' \<equiv> (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc 0))))))))))))"
abbreviation moving' :: variable where "moving' \<equiv> (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc (Suc 0)))))))))))))"

abbreviation Ctrl' :: process where "Ctrl' \<equiv> 1"

abbreviation STOP:: pstate where "STOP \<equiv> 0"
abbreviation ERROR:: pstate where "ERROR \<equiv> 1"

abbreviation Ctrl'goUp :: pstate where "Ctrl'goUp \<equiv> 3"
abbreviation Ctrl'goDown :: pstate where "Ctrl'goDown \<equiv> 4"
abbreviation Ctrl'stuckState :: pstate where "Ctrl'stuckState \<equiv> 5"
abbreviation Ctrl'emergency :: pstate where "Ctrl'emergency \<equiv> 6"
abbreviation Ctrl'motionless :: pstate where "Ctrl'motionless \<equiv> 2"

abbreviation UP' :: bool where "UP' \<equiv> True"
abbreviation DOWN' :: bool where "DOWN' \<equiv> False"
abbreviation DELAY' :: nat where "DELAY' \<equiv> 120000"
abbreviation SUSPENSION_TIME' :: nat where "SUSPENSION_TIME' \<equiv> 1000"

abbreviation DELAY'TIMEOUT :: nat where "DELAY'TIMEOUT \<equiv> 1200"
abbreviation SUSPENSION_TIME'TIMEOUT :: nat where "SUSPENSION_TIME'TIMEOUT \<equiv> 10"

datatype state =
  emptyState |
  toEnv state |
  setVarBool state variable bool |
  setVarInt state variable int |
  setVarReal state variable real |
  setVarArrayBool state variable int bool |
  setVarArrayInt state variable int int |
  setVarArrayReal state variable int real |
  setVarAny state bool bool bool bool bool |
  setPstate state process pstate |
  reset state process

fun getVarBool:: "state => variable => bool" where
  "getVarBool emptyState _ = False" |
  "getVarBool (toEnv s) x = getVarBool s x" |
  "getVarBool (setVarBool s y v) x = (if x = y then v else (getVarBool s x))" |
  "getVarBool (setVarInt s _ _) x = getVarBool s x" |
  "getVarBool (setVarReal s _ _) x = getVarBool s x" |
  "getVarBool (setVarArrayBool s _ _ _) x = getVarBool s x" |
  "getVarBool (setVarArrayInt s _ _ _) x = getVarBool s x" |
  "getVarBool (setVarArrayReal s _ _ _) x = getVarBool s x" |
  "getVarBool (setVarAny s  userAtTop''value userAtBottom''value directionSwitch''value alarmButton''value stuck''value) userAtTop' = userAtTop''value" |
  "getVarBool (setVarAny s  userAtTop''value userAtBottom''value directionSwitch''value alarmButton''value stuck''value) userAtBottom' = userAtBottom''value" |
  "getVarBool (setVarAny s  userAtTop''value userAtBottom''value directionSwitch''value alarmButton''value stuck''value) directionSwitch' = directionSwitch''value" |
  "getVarBool (setVarAny s  userAtTop''value userAtBottom''value directionSwitch''value alarmButton''value stuck''value) alarmButton' = alarmButton''value" |
  "getVarBool (setVarAny s  userAtTop''value userAtBottom''value directionSwitch''value alarmButton''value stuck''value) stuck' = stuck''value" |
  "getVarBool (setVarAny s  userAtTop''value userAtBottom''value directionSwitch''value alarmButton''value stuck''value) x = getVarBool s x" |
  "getVarBool (setPstate s _ _) x = getVarBool s x" |
  "getVarBool (reset s _) x = getVarBool s x"

