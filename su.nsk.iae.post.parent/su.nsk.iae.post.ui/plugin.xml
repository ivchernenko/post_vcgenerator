<?xml version="1.0" encoding="windows-1251"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
			contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
			default="true"
			extensions="post"
			icon="icons/PoST_file.png"
			id="su.nsk.iae.post.PoST"
			name="PoST Editor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="su.nsk.iae.post.PoST.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="su.nsk.iae.post.PoST.validate">
			<activeWhen>
				<reference
					definitionId="su.nsk.iae.post.PoST.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="su.nsk.iae.post.PoST.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="su.nsk.iae.post.PoST.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="su.nsk.iae.post.PoST.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="su.nsk.iae.post.PoST"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
		<definition id="su.nsk.iae.post.PoST.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="su.nsk.iae.post.PoST"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="su.nsk.iae.post.PoST"
			name="PoST">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
		</page>
		<page
			category="su.nsk.iae.post.PoST"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="su.nsk.iae.post.PoST.coloring"
			name="Syntax Coloring">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
		</page>
		<page
			category="su.nsk.iae.post.PoST"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="su.nsk.iae.post.PoST.templates"
			name="Templates">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="su.nsk.iae.post.PoST"
			name="PoST">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="su.nsk.iae.post.ui.keyword_PoST"
			label="PoST"/>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Trigger expensive validation"
			id="su.nsk.iae.post.PoST.validate"
			name="Validate">
		</command>
		<!-- copy qualified name -->
		<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
		</command>
		<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command
				commandId="su.nsk.iae.post.PoST.validate"
				style="push"
				tooltip="Trigger expensive validation">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="su.nsk.iae.post.PoST.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="su.nsk.iae.post.PoST.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="menu:edit?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="su.nsk.iae.post.PoST.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="su.nsk.iae.post.PoST.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
					<reference definitionId="su.nsk.iae.post.PoST.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
			commandId="org.eclipse.xtext.ui.editor.FindReferences">
			<activeWhen>
				<reference
					definitionId="su.nsk.iae.post.PoST.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type
			base-type="org.eclipse.core.runtime.text"
			file-extensions="post"
			id="su.nsk.iae.post.PoST.contenttype"
			name="PoST File"
			priority="normal">
		</content-type>
	</extension>
	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="post">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="post">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for su.nsk.iae.post.PoST -->
	<extension
			id="post.check.fast"
			name="PoST Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="post.check.normal"
			name="PoST Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="post.check.expensive"
			name="PoST Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="su.nsk.iae.post.PoST"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.validation.ValidatorPreferencePage"
			id="su.nsk.iae.post.PoST.validator.preferencePage"
			name="Errors/Warnings">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
		</page>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="post"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="su.nsk.iae.post.PoST"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="su.nsk.iae.post.PoST.compiler.preferencePage"
			name="Compiler">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.propertyPages">
		<page
			category="su.nsk.iae.post.PoST"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="su.nsk.iae.post.PoST.compiler.propertyPage"
			name="Compiler">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="su.nsk.iae.post.PoST.OpenGeneratedCode"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference definitionId="su.nsk.iae.post.PoST.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
			<activeWhen>
				<reference definitionId="su.nsk.iae.post.PoST.Editor.opened" />
			</activeWhen>
		</handler>
	</extension>
	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="su.nsk.iae.post.PoST.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Open the quick outline."
			id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
			name="Quick Outline">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="su.nsk.iae.post.PoST.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<!-- quickfix marker resolution generator for su.nsk.iae.post.PoST -->
	<extension
			point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="su.nsk.iae.post.ui.post.check.fast">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="su.nsk.iae.post.ui.post.check.normal">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="su.nsk.iae.post.ui.post.check.expensive">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
	</extension>
	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="su.nsk.iae.post.PoST.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="su.nsk.iae.post.PoST.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="su.nsk.iae.post.PoST"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
			id="su.nsk.iae.post.PoST.refactoring"
			name="Refactoring">
			<keywordReference id="su.nsk.iae.post.ui.keyword_PoST"/>
		</page>
	</extension>
	<extension point="org.eclipse.compare.contentViewers">
		<viewer id="su.nsk.iae.post.PoST.compare.contentViewers"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="post">
		</viewer>
		<contentTypeBinding
			contentTypeId="su.nsk.iae.post.PoST.contenttype"
			contentViewerId="su.nsk.iae.post.PoST.compare.contentViewers" />
	</extension>
	<extension point="org.eclipse.compare.contentMergeViewers">
		<viewer id="su.nsk.iae.post.PoST.compare.contentMergeViewers"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="post" label="PoST Compare">
		</viewer>
		<contentTypeBinding
			contentTypeId="su.nsk.iae.post.PoST.contenttype"
			contentMergeViewerId="su.nsk.iae.post.PoST.compare.contentMergeViewers" />
	</extension>
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider id="su.nsk.iae.post.PoST.editors.documentProviders"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
			extensions="post">
		</provider>
	</extension>
	<extension point="org.eclipse.team.core.fileTypes">
		<fileTypes
			extension="post"
			type="text">
		</fileTypes>
	</extension>
	<extension
		point="org.eclipse.ui.newWizards">
		<category id="su.nsk.iae.post.ui.category" name="PoST">
		</category>
		<wizard
			category="su.nsk.iae.post.ui.category"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.wizard.template.TemplateNewProjectWizard"
			id="su.nsk.iae.post.ui.wizard.PoSTNewProjectWizard"
			name="PoST Project"
			icon="icons/new_PoST_proj.png"
			project="true">
		</wizard>
	</extension>
	<extension
		point="org.eclipse.xtext.ui.projectTemplate">
		<projectTemplateProvider
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:su.nsk.iae.post.ui.wizard.PoSTProjectTemplateProvider"
			grammarName="su.nsk.iae.post.PoST">
		</projectTemplateProvider>
	</extension>
	<extension
		point="org.eclipse.ui.perspectiveExtensions">
		<perspectiveExtension targetID="org.eclipse.ui.resourcePerspective">
			<newWizardShortcut id="su.nsk.iae.post.ui.wizard.PoSTNewProjectWizard"/>
		</perspectiveExtension>
		<perspectiveExtension targetID="org.eclipse.jdt.ui.JavaPerspective">
			<newWizardShortcut id="su.nsk.iae.post.ui.wizard.PoSTNewProjectWizard"/>
		</perspectiveExtension>
	</extension>
	<extension
		point="org.eclipse.ui.menus">
		<menuContribution
			allPopups="false"
			locationURI="toolbar:org.eclipse.ui.main.toolbar">
			<toolbar
				id="su.nsk.iae.post.ui.toolbar">
				<!--
					For some reason the tooltip is not shown when hovering over the toolbar button
					See also https://www.eclipse.org/forums/index.php/t/1079111/
				-->
				<command
					commandId="org.eclipse.ui.newWizard"
					tooltip="Create a new PoST project">
					<parameter
						name="newWizardId"
						value="su.nsk.iae.post.ui.wizard.PoSTNewProjectWizard">
					</parameter>
				</command>
			</toolbar>
		</menuContribution>
	</extension>
	<extension
		point="org.eclipse.ui.newWizards">
		<category id="su.nsk.iae.post.ui.category" name="PoST">
		</category>
		<wizard
			category="su.nsk.iae.post.ui.category"
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:org.eclipse.xtext.ui.wizard.template.TemplateNewFileWizard"
			id="su.nsk.iae.post.ui.wizard.PoSTNewFileWizard"
			name="PoST File"
			icon="icons/new_PoST_file.png">
		</wizard>
	</extension>
	<extension
		point="org.eclipse.xtext.ui.fileTemplate">
		<fileTemplateProvider
			class="su.nsk.iae.post.ui.PoSTExecutableExtensionFactory:su.nsk.iae.post.ui.wizard.PoSTFileTemplateProvider"
			grammarName="su.nsk.iae.post.PoST">
		</fileTemplateProvider>
	</extension>
	<extension
		point="org.eclipse.ui.perspectiveExtensions">
		<perspectiveExtension targetID="org.eclipse.ui.resourcePerspective">
			<newWizardShortcut id="su.nsk.iae.post.ui.wizard.PoSTNewFileWizard"/>
		</perspectiveExtension>
		<perspectiveExtension targetID="org.eclipse.jdt.ui.JavaPerspective">
			<newWizardShortcut id="su.nsk.iae.post.ui.wizard.PoSTNewFileWizard"/>
		</perspectiveExtension>
	</extension>
	<extension
		point="org.eclipse.ui.menus">
		<menuContribution
			allPopups="false"
			locationURI="toolbar:org.eclipse.ui.main.toolbar">
			<toolbar
				id="su.nsk.iae.post.ui.toolbar">
				<!--
					For some reason the tooltip is not shown when hovering over the toolbar button
					See also https://www.eclipse.org/forums/index.php/t/1079111/
				-->
				<command
					commandId="org.eclipse.ui.newWizard"
					tooltip="Create a new PoST file">
					<parameter
						name="newWizardId"
						value="su.nsk.iae.post.ui.wizard.PoSTNewFileWizard">
					</parameter>
				</command>
			</toolbar>
		</menuContribution>
	</extension>
</plugin>
